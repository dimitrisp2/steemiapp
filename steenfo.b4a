		TotalAuthSBD = TotalAuthSBD + SBD.Replace(" SBD", "")
		TotalAuthSP = TotalAuthSP + Round2(SP, 3)
		
		uAuth.Add(SBD.Replace(" ", "") & STEEM & ", " & Round2(SP, 3) & "SP - " & timestamp & "^$^" & PermLink & "^$^" & TXType & "^" & txID & "^" &BlockID & "^" & Author & "/" & PermLink)
		
