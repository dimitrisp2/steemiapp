			End If
		Else ' votescount 6 or less
			If thisvote = 1 Then
				ChosenUpvotedBy = voter
			Else
				ChosenUpvotedBy = voter & ", " & ChosenUpvotedBy
			End If
		End If
	Next
	Dim author As String = result.Get("author")
	Dim created As String = result.Get("created")
